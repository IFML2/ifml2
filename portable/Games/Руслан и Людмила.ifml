        <procedure name="осмотреть Руслана">
            <procedureVariables/>
            <procedureBody>
                <showMessage type="text"
                             message="&#xD;&#xA;- Ну, прямо всем хочется осмотреть Руслана.&#xD;&#xA;- Вот он, молодой, красивый, здоровый, как бык. Начинающий, но подающий, как говорят, большие надежды актер.&#xD;&#xA;- Узнали в гриме? На уши обратите внимание.&#xD;&#xA;"
                             carriageReturn="true" beginWithCap="false"/>
            </procedureBody>
            <parameters/>
        </procedure>
        <procedure name="ждать">
            <procedureVariables/>
            <procedureBody>
                <showMessage type="text"
