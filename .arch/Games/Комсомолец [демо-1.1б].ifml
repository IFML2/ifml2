                <parameter name="скем"/>
                <parameter name="вопрос"/>
            </parameters>
        </procedure>
        <procedure name="ждать">
            <procedureVariables/>
            <procedureBody>
                <if condition="Цицея = 1">
                    <else/>
                    <then>
                        <if condition="объявлена-посадка">
                            <else>
                                <if condition="уботпред в ТекущаяЛокация.предметы">
                                    <else/>
                                    <then>
                                        <moveItem to="СкладПредметовЛок.предметы" item="УботПред"/>
                                    </then>
                                </if>
                                <if condition="робоноспред в ТекущаяЛокация.предметы">
                                    <else/>
                                    <then>
                                        <moveItem to="СкладПредметовЛок.предметы" item="РобоносПред"/>
                                    </then>
                                </if>
                                <if condition="куботпред в ТекущаяЛокация.предметы">
                                    <else/>
                                    <then>
                                        <moveItem to="СкладПредметовЛок.предметы" item="КуботПред"/>
                                    </then>
                                </if>
                                <var value="да" name="объявлена-посадка"/>
                                <showMessage type="text"
                                             message="Приятный негромкий голос женщины-администратора словно эхом отразившись от стен купола возвращает меня в реальность:&#xA;&#xA;&#x9;&quot;Пассажирам на рейс &quot;Меркурий-1 - Земля-Центральная&quot; просьба пройти к Терминалу Б&quot;.&#xA;&#xA;Кто-то положил мне руку на плечо...&#xA;Пара карих изучающих глаз, казалось, оценивали меня. Курносый нос, со следами веснушек. Ее длинные волосы свободно спадали на плечи. В легкой ухмылке, казалось, боролись пренебрежение и отчаяние. Победило второе.&#xA;&#x9;- Ты летишь на Землю, да?&#xA;Что я мог ответить, она поймала меня с поличным.&#xA;&#x9;- Слушай, парень, выручи меня, пойдем к Администратору, скажешь, что я лечу с тобой. Понимаешь,... я потеряла карту работника, а без нее мне не дадут билет. А мне очень, очень надо попасть на Землю! Что скажешь, поможешь мне? Кстати, меня зовут Евгения.&#xA;(наберите &lt;да&gt;, если вы согласны помочь Евгении или &lt;нет&gt; - если вы отказываетесь)"
                                             carriageReturn="true" beginWithCap="false"/>
                                <moveItem to="ТекущаяЛокация.предметы" item="ЕвгенияПред"/>
                                <var value="'На широком экране терминала яркими желтыми буквами мигает надпись: Меркурий-1 - Земля-Центральная -----&gt; ИДЕТ ПОСАДКА'"
                                     name="текстПанели"/>
                                <moveItem to="СкладПредметовЛок.предметы" item="ДедушкаФедиПред"/>
                                <moveItem to="ШаттлЛок.предметы" item="ФедяПред"/>
                                <runProcedure returnToVar="" procedure="очистка ленты транспортера"/>
                            </else>
                            <then>
                                <showMessage type="text" message="У меня нет времени ждать, мне надо попасть на шаттл."
                                             carriageReturn="true" beginWithCap="false"/>
                            </then>
                        </if>
                    </then>
                </if>
            </procedureBody>
            <parameters/>
        </procedure>
        <procedure name="вставить">
            <procedureVariables/>
            <procedureBody>
                <showMessage type="expression" message="'Не стоит пытаться ' + глагол + ' ' + вещь1.вп + ' в ' + вещь2.вп + '.'"
                             carriageReturn="true" beginWithCap="false"/>
            </procedureBody>
            <parameters>
                <parameter name="вещь1"/>
                <parameter name="вещь2"/>
                <parameter name="глагол"/>
            </parameters>
        </procedure>
        <procedure name="идти в зал ожидания">
            <procedureVariables/>
            <procedureBody>
                <if condition="Цицея = 1">
                    <else>
                        <showMessage type="text" message="Это невозможно." carriageReturn="true" beginWithCap="false"/>
                    </else>
                    <then>
                        <goToLoc location="Зал"/>
                        <var value="0" name="сидит"/>
                        <var value="нет" name="сидитрефлексатор"/>
                        <if condition="уботпред в СкладпредметовЛок.предметы">
                            <else>
                                <moveItem to="СкладПредметовЛок.предметы" item="УботПред"/>
                            </else>
                            <then/>
                        </if>
                        <if condition="робоноспред в СкладПредметовЛок.предметы">
                            <else>
                                <moveItem to="СкладПредметовЛок.предметы" item="РобоносПред"/>
                            </else>
                            <then/>
                        </if>
                        <if condition="куботпред в СкладПредметовЛок.предметы">
                            <else>
                                <moveItem to="СкладПредметовЛок.предметы" item="КуботПред"/>
                            </else>
                            <then/>
                        </if>
                        <rollDice var="вероятностьробота" to="6" from="1"/>
                        <if condition="вероятностьробота = 1">
                            <else>
                                <if condition="вероятностьробота = 2">
                                    <else>
                                        <if condition="вероятностьробота = 3">
                                            <else/>
                                            <then>
                                                <runProcedure returnToVar="" procedure="убот возвращает вещь"/>
                                                <showMessage type="text" message="Убот методично очищает пол." carriageReturn="true"
                                                             beginWithCap="false"/>
                                                <moveItem to="ТекущаяЛокация.предметы" item="УботПред"/>
                                            </then>
                                        </if>
                                    </else>
                                    <then>
                                        <showMessage type="text" message="Высоко над головой проносится кубот." carriageReturn="true"
                                                     beginWithCap="false"/>
                                        <moveItem to="ТекущаяЛокация.предметы" item="КуботПред"/>
                                    </then>
                                </if>
                            </else>
                            <then>
                                <showMessage type="text" message="Мимо неспешно проезжает робот-носильщик." carriageReturn="true"
                                             beginWithCap="false"/>
                                <moveItem to="ТекущаяЛокация.предметы" item="РобоносПред"/>
                            </then>
                        </if>
                        <if condition="помогЕвгении">
                            <else/>
                            <then>
                                <showMessage type="text" message="Евгения идет со мной." carriageReturn="true" beginWithCap="false"/>
                                <moveItem to="ТекущаяЛокация.предметы" item="ЕвгенияПред"/>
                            </then>
                        </if>
                        <var value="нет" name="гейтОткрыт"/>
                        <var value="нет" name="разговор-админ"/>
                        <var value="'0'" name="разряд1"/>
                        <var value="'0'" name="разряд2"/>
                        <var value="'0'" name="разряд3"/>
                        <var value="'0'" name="разряд4"/>
                        <var value="'0'" name="разряд5"/>
                        <var value="'0'" name="разряд6"/>
                        <var value="'0'" name="разряд7"/>
                        <var value="'0'" name="разряд8"/>
                        <var value="'0'" name="разряд9"/>
                        <var value="'0'" name="разряд10"/>
                        <var value="0" name="счетчикРазрядов"/>
                    </then>
                </if>
            </procedureBody>
            <parameters/>
        </procedure>
