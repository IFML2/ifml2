        <procedure name="запад">
            <procedureVariables/>
            <procedureBody>
                <var value="нет" name="сидитрефлексатор"/>
                <var value="0" name="сидит"/>
                <if condition="уботпред в СкладПредметовЛок.предметы">
                    <else>
                        <moveItem to="СкладПредметовЛок.предметы" item="УботПред"/>
                    </else>
                    <then/>
                </if>
                <if condition="робоноспред в СкладПредметовЛок.предметы">
                    <else>
                        <moveItem to="СкладПредметовЛок.предметы" item="РобоносПред"/>
                    </else>
                    <then/>
                </if>
                <if condition="куботпред в СкладПредметовЛок.предметы">
                    <else>
                        <moveItem to="СкладПредметовЛок.предметы" item="КуботПред"/>
                    </else>
                    <then/>
                </if>
                <goToLoc location="ТекущаяЛокация.запад"/>
                <rollDice var="вероятностьробота" to="6" from="1"/>
                <if condition="вероятностьробота = 1">
                    <else>
                        <if condition="вероятностьробота = 2">
                            <else>
                                <if condition="вероятностьробота = 3">
                                    <else/>
                                    <then>
                                        <runProcedure returnToVar="" procedure="убот возвращает вещь"/>
                                        <showMessage type="text" message="Убот методично очищает пол." carriageReturn="true"
                                                     beginWithCap="false"/>
                                        <moveItem to="ТекущаяЛокация.предметы" item="УботПред"/>
                                    </then>
                                </if>
                            </else>
                            <then>
                                <showMessage type="text" message="Высоко над головой проносится кубот." carriageReturn="true"
                                             beginWithCap="false"/>
                                <moveItem to="ТекущаяЛокация.предметы" item="КуботПред"/>
                            </then>
