    <procedures>
        <procedure name="Идти к пульту">
            <procedureVariables/>
            <procedureBody>
                <if condition="Цицея = 1">
                    <else>
                        <showMessage type="text" message="Это невозможно." carriageReturn="true" beginWithCap="false"/>
                    </else>
                    <then>
                        <goToLoc location="ПультАдминистратораЛок"/>
                        <var value="нет" name="сидитрефлексатор"/>
                        <var value="0" name="сидит"/>
                        <if condition="уботпред в СкладПредметовЛок.предметы">
                            <else>
                                <moveItem to="СкладПредметовЛок.предметы" item="УботПред"/>
                            </else>
                            <then/>
                        </if>
                        <if condition="робоноспред в СкладПредметовЛок.предметы">
                            <else>
                                <moveItem to="СкладПредметовЛок.предметы" item="РобоносПред"/>
                            </else>
                            <then/>
                        </if>
                        <if condition="куботпред в СкладПредметовЛок.предметы">
                            <else>
                                <moveItem to="СкладПредметовЛок.предметы" item="КуботПред"/>
                            </else>
                            <then/>
                        </if>
                        <rollDice var="вероятностьробота" to="6" from="1"/>
                        <if condition="вероятностьробота = 1">
                            <else>
                                <if condition="вероятностьробота = 2">
                                    <else>
                                        <if condition="вероятностьробота = 3">
                                            <else/>
                                            <then>
                                                <runProcedure returnToVar="" procedure="убот возвращает вещь"/>
                                                <showMessage type="text" message="Убот методично очищает пол." carriageReturn="true"
                                                             beginWithCap="false"/>
                                                <moveItem to="ТекущаяЛокация.предметы" item="УботПред"/>
                                            </then>
                                        </if>
                                    </else>
                                    <then>
                                        <showMessage type="text" message="Высоко над головой проносится кубот." carriageReturn="true"
                                                     beginWithCap="false"/>
                                        <moveItem to="ТекущаяЛокация.предметы" item="КуботПред"/>
                                    </then>
                                </if>
                            </else>
                            <then>
                                <showMessage type="text" message="Мимо неспешно проезжает робот-носильщик." carriageReturn="true"
                                             beginWithCap="false"/>
                                <moveItem to="ТекущаяЛокация.предметы" item="РобоносПред"/>
                            </then>
                        </if>
                        <if condition="помогЕвгении">
                            <else/>
                            <then>
                                <showMessage type="text" message="Евгения идет со мной." carriageReturn="true" beginWithCap="false"/>
                                <moveItem to="ТекущаяЛокация.предметы" item="ЕвгенияПред"/>
                            </then>
                        </if>
                        <var value="нет" name="гейтОткрыт"/>
                        <var value="нет" name="разговор-админ"/>
                    </then>
                </if>
            </procedureBody>
            <parameters/>
        </procedure>
        <procedure name="СестьПредмет">
            <procedureVariables/>
            <procedureBody>
                <if condition="предмет = РефлексаторПред">
                    <else>
                        <showMessage type="expression" message="'Я не буду садиться на ' + предмет.вп + '.'" carriageReturn="true"
                                     beginWithCap="true"/>
                    </else>
                    <then>
                        <if condition="объявлена-посадка">
                            <else>
                                <if condition="сидит = 0">
                                    <else>
                                        <showMessage type="text"
                                                     message="Я устраиваюсь поудобнее...&#xA;Закрываю глаза...&#xA;Успокаивающие импульсы пробегают по позвоночнику...&#xA;&#xA;Где-то далеко отсюда приятный голос объявляет про какую-то посадку...&#xA;&#xA;Кто-то положил мне руку на плечо...&#xA;Пара карих изучающих глаз, казалось, оценивали меня. Курносый нос, со следами веснушек. Ее длинные волосы свободно спадали на плечи. В легкой ухмылке, казалось, боролись пренебрежение и отчаяние. Победило второе.&#xA; - Не разбудила?&#xA;Что я мог ответить, она поймала меня с поличным.&#xA;- Слушай, парень, выручи меня, пойдем к Администратору, скажешь, что я лечу с тобой. Понимаешь,... я потеряла карту работника, а без нее мне не дадут билет. А мне очень, очень надо попасть на Землю! Что скажешь, поможешь мне? Кстати, меня зовут Евгения.&#xA;(наберите &lt;да&gt;, если вы согласны помочь Евгении или &lt;нет&gt; - если вы отказываетесь)"
                                                     carriageReturn="true" beginWithCap="false"/>
                                        <moveItem to="ТекущаяЛокация.предметы" item="ЕвгенияПред"/>
                                        <var value="да" name="объявлена-посадка"/>
                                        <moveItem to="СкладПредметовЛок.предметы" item="ДедушкаФедиПред"/>
                                        <moveItem to="ШаттлЛок.предметы" item="ФедяПред"/>
                                        <var value="'На широком экране терминала яркими желтыми буквами мигает надпись: Меркурий-1 - Земля-Центральная -----&gt; ИДЕТ ПОСАДКА'"
                                             name="текстПанели"/>
                                        <runProcedure returnToVar="" procedure="очистка ленты транспортера"/>
                                    </else>
                                    <then>
                                        <showMessage type="text"
                                                     message="Я с удовольствием растягиваюсь на рефлексаторе. Медленные, едва ощутимые согревающие волны, ласкают и успокаивают мое тело. Впрочем, заснуть скорее всего, не получится, а вот расслабиться вполне возможно. Специально разработанный и запрограммированный массаж заставляет чувствовать себя посвежевшим."
                                                     carriageReturn="true" beginWithCap="false"/>
                                        <var value="1" name="сидит"/>
                                        <var value="да" name="сидитрефлексатор"/>
                                        <rollDice var="убот-появился" to="1" from="0"/>
                                        <if condition="убот-появился = 1">
                                            <else/>
                                            <then>
                                                <if condition="уботпред в ТекущаяЛокация.предметы">
                                                    <else/>
                                                    <then>
                                                        <moveItem to="СкладПредметовЛок.предметы" item="УботПред"/>
                                                    </then>
                                                </if>
                                                <if condition="буклетпред1 в инвентарий">
                                                    <else>
                                                        <showMessage type="text"
                                                                     message="Пока я сидел ко мне подъехал убот, похожий на перевернутый отполированный таз."
                                                                     carriageReturn="true" beginWithCap="false"/>
                                                        <moveItem to="ТекущаяЛокация.предметы" item="УботПред"/>
                                                    </else>
                                                    <then>
                                                        <showMessage type="text"
                                                                     message="Я и не заметил как подъехал убот, пока что-то твердое не врезалось в мой ботинок. Я наклонился посмотреть, а в следующий момент длинная щупальца-манипулятор выхватила буклет из моих рук.&#xA;- Эй!&#xA;Только и успел я крикнуть. Блестящие створки панциря робота на мгновение раскрылись и брошюра навсегда исчезла из этого мира."
                                                                     carriageReturn="true" beginWithCap="false"/>
                                                        <moveItem to="ТекущаяЛокация.предметы" item="УботПред"/>
                                                        <moveItem to="стойкаПред.предметысверху" item="БуклетПред1"/>
                                                    </then>
                                                </if>
                                            </then>
                                        </if>
                                    </then>
                                </if>
                            </else>
                            <then>
                                <showMessage type="text" message="Некогда нежиться в кресле, надо попасть на рейс." carriageReturn="true"
                                             beginWithCap="false"/>
                            </then>
                        </if>
                    </then>
                </if>
            </procedureBody>
            <parameters>
                <parameter name="предмет"/>
            </parameters>
        </procedure>
        <procedure name="войти">
            <procedureVariables/>
            <procedureBody>
                <showMessage type="text" message="Не представляю как это сделать." carriageReturn="true" beginWithCap="true"/>
            </procedureBody>
            <parameters>
                <parameter name="предмет"/>
            </parameters>
        </procedure>
        <procedure name="встать">
            <procedureVariables/>
            <procedureBody>
                <if condition="сидит = 1">
                    <else>
                        <showMessage type="text" message="Уже стою." carriageReturn="true" beginWithCap="false"/>
                    </else>
                    <then>
                        <if condition="сидитрефлексатор = да">
                            <else/>
                            <then>
                                <showMessage type="text" message="Я с неохотой высвобождаюсь из приятных объятий рефлексатора."
                                             carriageReturn="true" beginWithCap="false"/>
                                <var value="0" name="сидит"/>
                                <var value="нет" name="сидитрефлексатор"/>
                            </then>
                        </if>
                    </then>
                </if>
            </procedureBody>
            <parameters/>
        </procedure>
        <procedure name="говорить">
