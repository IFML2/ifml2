    <locations>
        <location id="ЦентрЛок" name="Центральная часть храма">
            <hooks>
                <hook action="осмотреться" objectElement="" type="before">
                    <instructions>
                        <showPicture filepath="Картинки\Центр.jpg" maxHeight="0" maxWidth="0"/>
                        <showMessage beginWithCap="false" carriageReturn="true" message="" type="text"/>
                    </instructions>
                </hook>
                <hook action="идти на север" objectElement="" type="instead">
                    <instructions>
                        <if condition="факелПред в инвентарий">
                            <else>
                                <showMessage beginWithCap="false" carriageReturn="true" message="Слишком темно, ничего не видно." type="text"/>
                            </else>
                            <then>
                                <showMessage beginWithCap="false" carriageReturn="true" message="Ты пошел на север." type="text"/>
                                <goToLoc location="СеверЛок"/>
                            </then>
                        </if>
                    </instructions>
                </hook>
                <hook action="идти на восток" objectElement="" type="instead">
                    <instructions>
                        <if condition="факелПред в инвентарий">
                            <else>
                                <showMessage beginWithCap="false" carriageReturn="true" message="Слишком темно, ничего не видно." type="text"/>
                            </else>
                            <then>
                                <showMessage beginWithCap="false" carriageReturn="true" message="Ты пошел на восток." type="text"/>
                                <goToLoc location="ВостокЛок"/>
                            </then>
                        </if>
                    </instructions>
                </hook>
                <hook action="идти на юг" objectElement="" type="instead">
