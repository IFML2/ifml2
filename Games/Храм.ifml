<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<story xmlns="http://toysoft.narod.ru/IFML2Schema" id="story">
    <inheritedSystemProcedures/>
    <storyOptions>
        <startLocationOption showStartLocDesc="true"/>
        <startProcedureOption procedure="стартовая"/>
        <storyDescription name="Тайна затерянного храма" description="Игра портирована из платформы Адрифт." version="1.1" author="Андрей Репин"/>
        <globalVars>
            <var name="алтарь" value="1"/>
            <var name="броситьКувшин" value="нет"/>
        </globalVars>
        <systemCommandsDisableOption DisableHelp="false" DisableDebug="false"/>
    </storyOptions>
    <procedures>
        <procedure name="стартовая">
            <procedureVariables/>
            <procedureBody>
                <showMessage beginWithCap="false" carriageReturn="true" message="Ужасно болит голова. Эта боль и заставляет тебя очнуться от забытья. Ты лежишь на чем-то жестком и холодном. Стараясь что-то вспомнить, ты открываешь глаза. &quot;Где я?&quot;&#xA;&#xA;&#xA;Ты лежишь на полу в незнакомом месте." type="text"/>
            </procedureBody>
            <parameters/>
        </procedure>
        <procedure name="осмотреть себя">
            <procedureVariables/>
            <procedureBody>
                <showMessage beginWithCap="false" carriageReturn="true" message="Ты осмотрел себя. Видимых повреждений нет. Но как ты здесь очутился, не помнишь." type="text"/>
                <if condition="шарфПред.надет  = да">
                    <else/>
                    <then>
                        <showMessage beginWithCap="false" carriageReturn="true" message="На шее шарф." type="text"/>
                        <if condition="шарфПред в инвентарий">
                            <else>
                                <moveItem item="ШарфПред" to="инвентарий"/>
                            </else>
                            <then/>
                        </if>
                    </then>
                </if>
            </procedureBody>
            <parameters/>
        </procedure>
        <procedure name="положить кувшин">
            <procedureVariables/>
            <procedureBody>
                <showMessage beginWithCap="false" carriageReturn="true" message="Ты поставил кувшин на пол." type="text"/>
                <moveItem item="КувшинПред" to="ТекущаяЛокация.предметы"/>
            </procedureBody>
            <parameters/>
        </procedure>
        <procedure name="сделать факел">
            <procedureVariables/>
            <procedureBody>
                <showMessage beginWithCap="false" carriageReturn="true" message="Ты обмазал шарф смолой и намотал его на палку. Получился факел." type="text"/>
                <moveItem item="ПалкаПред" to="ПрикупЛок.предметы"/>
                <moveItem item="ШарфПред" to="ПрикупЛок.предметы"/>
                <moveItem item="СмолаПред" to="ПрикупЛок.предметы"/>
                <moveItem item="СделанныйТобоюФакелПред" to="инвентарий"/>
            </procedureBody>
            <parameters/>
        </procedure>
        <procedure name="потушить свечу">
            <procedureVariables/>
            <procedureBody>
                <showMessage beginWithCap="false" carriageReturn="true" message="Ну, вот, ты потушил свечу." type="text"/>
                <goToLoc location="ТемнаяЛок"/>
            </procedureBody>
            <parameters/>
        </procedure>
        <procedure name="ПоменятьПредметы">
            <procedureVariables/>
            <procedureBody>
                <var name="КоллОдного" value="ОдинПредмет.СодержащаяКоллекция"/>
                <var name="КоллДругого" value="ДругойПредмет.СодержащаяКоллекция"/>
                <moveItem item="ОдинПредмет" to="КоллДругого"/>
                <moveItem item="ДругойПредмет" to="КоллОдного"/>
            </procedureBody>
            <parameters>
                <parameter name="ОдинПредмет"/>
                <parameter name="ДругойПредмет"/>
            </parameters>
        </procedure>
    </procedures>
